{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
         "no-plusplus":"off",
         "import/no-unresolved":"off",
         "max-len":["error", 135],
         "class-methods-use-this":"off",
         "no-shadow":"warn",
         "consistent-return":"off",
         "no-unused-vars":"warn",
         "no-nested-ternary":"off"
    }
}
